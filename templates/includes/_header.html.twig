<div class="uikit-global-nav">
    <div class="container">
        {{ page.nav_secondary }}
    </div>
</div>
<header id="header" role="banner" class="navbar">
    <div class="container">
        <div class="row">
            <nav class="navbar">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    {{ page.header }}
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#main-navigation" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
            </nav>
        </div>
    </div>
</header>

<div class="container-full main-nav">
    <div class="container">
        <div class="row">
            <div class="collapse navbar-collapse" id="main-navigation">
                {{ page.nav }}
            </div>
        </div><!-- /.navbar-collapse -->
    </div>
</div>
{% if page.highlighted and page.highlighted|render|striptags|trim %}
    <div class="jumbotron">
        <div id="highlight-region" class="container">
            {{ page.highlighted }}
        </div>
    </div>
{% endif %}